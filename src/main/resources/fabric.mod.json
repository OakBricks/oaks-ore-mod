  "description": "very simple ore mod for fabric 1.17, remake of my mcreator mod",
  "authors": [
    "OakBricks"
  ],
  "contact": {
    "homepage": "https://github.com/OakBricks/oaks-ore-mod",
    "sources": "https://github.com/OakBricks/oaks-ore-mod"
  },

  "license": "GPL-3.0",
  "icon": "assets/oakores/icon.png",

  "environment": "*",
